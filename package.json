{
  "name": "automap",
  "version": "0.1.0",
  "type": "module",
  "description": "tiny and schemaless Redis ORM-\"ish\" microframework",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test node --test test/unit/index.spec.js",
    "test:perf": "NODE_ENV=test node --test test/perf/index.spec.js",
    "test:coverage": "NODE_ENV=test node --test --experimental-test-coverage test/index.spec.js",
    "test:precommit": "npm test && npm audit && npm run lint",

    "lint": "npx eslint",
    "scratch": "node .github/scratch/index.js",
    "example": "node .github/example/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "os": ["darwin", "linux"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswmin/automap.git"
  },
  "keywords": [
    "orm",
    "object-mapping",
    "object-relational-mapping",
    "redis",
    "javascript",
    "database"
  ],
  "author": "@nicholaswmin",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/nicholaswmin/automap/issues"
  },
  "homepage": "https://github.com/nicholaswmin/automap#readme",
  "dependencies": {
    "dot-object": "^2.1.5",
    "object-traversal": "^1.0.1",
    "pathval": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "benchmrk": "^0.1.3",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0"
  }
}
