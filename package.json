{
  "name": "automap",
  "version": "0.1.0",
  "type": "module",
  "description": "tiny and schemaless Redis ORM-\"ish\" microframework",
  "main": "index.js",
  "scripts": {
    "test": "node --test test/index.spec.js",
    "test:coverage": "NODE_ENV=test node --test --experimental-test-coverage test/index.spec.js",
    "test:integration": "npm run ping-redis && NODE_ENV=test node --test test/integration/index.spec.js",
    "test:performance": "npm run ping-redis && NODE_ENV=test node --test test/performance/index.spec.js",
    "checks": "npx eslint && npm audit && npx size-limit",
    "example": "node .github/example/index.js",
    "benchmark": "node .github/scratch/index.js",
    "ping-redis": "node test/helpers/ping-redis/index.js"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "5 kB"
    }
  ],
  "engines": {
    "node": ">=20"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholaswmin/automap.git"
  },
  "keywords": [
    "orm",
    "object-mapping",
    "object-relational-mapping",
    "redis",
    "javascript",
    "database"
  ],
  "author": "@nicholaswmin",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/nicholaswmin/automap/issues"
  },
  "homepage": "https://github.com/nicholaswmin/automap#readme",
  "dependencies": {
    "dot-object": "^2.1.5",
    "object-traversal": "^1.0.1",
    "pathval": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "benchmrk": "^0.1.3",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "size-limit": "^11.1.4",
    "throttleit": "^2.1.0"
  }
}
